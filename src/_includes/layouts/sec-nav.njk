<!doctype html>
<html class="no-js" lang="en">
  <head>
    {% include "layouts/head.njk" %}
  </head>
  <body>
    <header>
      {% include "blocks/top-nav.njk" %}
    </header>
    <div id="siteBox">
      <h1 class="title-heading">{{ title }}</h1>
      <nav id="secNav">
        <ul>
          {% for lvl1Item in navTree %}
            {% if lvl1Item.lvl1Item == secNavSec %}
              {% for lvl2Item in lvl1Item.lvl2Items %}
                <li class="nav-dropdown-0">
                  <a href="{{ lvl2Item.lvl2Link }}" class="sidenav-lvl1">
                    <span>{{ lvl2Item.lvl2Label }}</span></a>
                </li>
              {% endfor %}
            {% endif %}
          {% endfor %}
        </ul>
      </nav>
      <main>{{ content | safe }}</main>
    </div>
    <footer>
    </footer>
    {% include "layouts/scripts.njk" %}
  </body>
</html>